{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Entry",
  "description": "A bucket list entry",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for an entry",
      "type": "number"
    },
    "title": {
      "description": "Title of the entry",
      "type": "string"
    },
    "content": {
      "description": "Content of the entry",
      "type": "string"
    },
    "selected": {
      "description": "Whether the entry is selected",
      "type": "boolean"
    }
  },
  "required": ["id", "title", "content"]
}